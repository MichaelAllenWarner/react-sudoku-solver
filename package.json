{
  "name": "sudoku-react",
  "version": "1.0.0",
  "description": "React version of my sudoku-solver web app",
  "engines": {
    "node": "12.3.1"
  },
  "main": "server/server.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "webpack -p",
    "start": "node --experimental-modules --require dotenv/config server/server.js",
    "dev-client": "webpack-dev-server -d --hot --host 0.0.0.0",
    "dev-server": "nodemon --experimental-modules --require dotenv/config server/server.js",
    "dev": "concurrently \"npm run dev-server\" \"npm run dev-client\""
  },
  "type": "module",
  "author": "Michael Allen Warner",
  "license": "ISC",
  "dependencies": {
    "@hot-loader/react-dom": "^16.8.6",
    "body-parser": "^1.19.0",
    "compression": "^1.7.4",
    "core-js": "^3.1.4",
    "dotenv": "^8.0.0",
    "express": "^4.17.1",
    "mongoose": "^5.6.5",
    "mongoose-unique-validator": "^2.0.3",
    "promise-polyfill": "^8.1.3",
    "prop-types": "^15.7.2",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-hot-loader": "^4.12.10",
    "whatwg-fetch": "^3.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "@babel/preset-react": "^7.0.0",
    "babel-eslint": "^10.0.2",
    "babel-loader": "^8.0.6",
    "concurrently": "^4.1.1",
    "css-loader": "^3.1.0",
    "eslint": "^5.16.0",
    "eslint-plugin-react": "^7.14.2",
    "html-webpack-exclude-assets-plugin": "0.0.7",
    "html-webpack-plugin": "^3.2.0",
    "mini-css-extract-plugin": "^0.8.0",
    "nodemon": "^1.19.1",
    "style-loader": "^0.23.1",
    "webpack": "^4.36.1",
    "webpack-cli": "^3.3.6",
    "webpack-dev-server": "^3.7.2",
    "worker-loader": "^2.0.0"
  }
}
